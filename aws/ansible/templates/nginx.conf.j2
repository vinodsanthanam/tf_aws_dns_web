server {
    listen 80;
    listen 443 ssl;
 
    root /var/www/home/;
 
    index index.html index.htm;
 
    server_name {{ server_name }};
    ssl_certificate {{ cert_file }};
    ssl_certificate_key {{ key_file }};
    location / {
        default_type "text/html";        
        try_files $uri $uri/ =404;
    }
}